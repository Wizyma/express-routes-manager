    "reporter": [
      "text"
    ]
  },
  "author": "Andr√© Gomes",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/node": "^10.12.9",
    "cpy-cli": "^1.0.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "source-map-support": "^0.5.4",
    "ts-node": "^6.0.0",
